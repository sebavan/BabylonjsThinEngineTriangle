{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Launch Index (Chrome)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true,
            "preLaunchTask": "devServer",
            "userDataDir": "${workspaceRoot}/.tempChromeProfileForDebug",
            "runtimeArgs": [
                "--enable-unsafe-es3-apis"
            ]
        }
    ]
}